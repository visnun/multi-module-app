<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:table='urn:import:com.google.gwt.user.cellview.client'>


    <g:HTMLPanel>
        <div class="container">
            <table:CellTable ui:field="table"/>

            <div class="button-container">
                <g:Button ui:field="createButton"/>

                <g:Button ui:field="deleteButton"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>